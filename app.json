{
  "expo": {
    "scheme": "saner",
    "plugins": [
      "expo-router",
      "expo-speech-recognition"
    ],
    "name": "Saner",
    "slug": "Saner",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.almas_cp.Saner"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      {
        "asset": "assets/fonts/Montserrat-Bold.ttf",
        "family": "Montserrat-Bold"
      }
    ]
  }
}
